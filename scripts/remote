on *:TEXT:!Fupa*:#:{
  /set %Fupa $rand(1,10) 
  if (%Fupa == 1) .msg $chan 10No! 
  if (%Fupa == 2) .msg $chan 10Yes 
  if (%Fupa == 3) .msg $chan 10Maybe 
  if (%Fupa == 4) /msg $chan 10Go fuck yourself
  if (%Fupa == 5) .msg $chan 10of course 
  if (%Fupa == 6) .msg $chan 10Are you crazy?!?! 
  if (%Fupa == 7) /msg $chan 10How should I know? 
  if (%Fupa == 8) /msg $chan 10That's a ghey question
  if (%Fupa == 9) /msg $chan 10Bacon
  if (%Fupa == 10) /msg $chan 10That's what she said 
}
